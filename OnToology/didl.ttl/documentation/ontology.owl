        <rdfs:label xml:lang="en">assertedAttribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#assertion -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#assertion">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Assertion"/>
