    <!-- https://w3id.org/def/didl#assertedAttribute -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#assertedAttribute">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Attribute"/>
        <rdfs:comment xml:lang="en">Indicates that an assertion has asserted attribute(s)</rdfs:comment>
        <rdfs:label xml:lang="en">assertedAttribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#assertion -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#assertion">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:comment>Indicates that an identity subject (i.e., entity) has assertion(s)</rdfs:comment>
        <rdfs:label>assertion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#assertionFormat -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#assertionFormat">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#AssertionFormat"/>
        <rdfs:comment xml:lang="en">Indicates that an assertion has its specific format</rdfs:comment>
        <rdfs:label xml:lang="en">assertionFormat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#controlledBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#controlledBy">
        <owl:inverseOf rdf:resource="https://w3id.org/def/didl#controlls"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:comment xml:lang="en">Indicates an identity is controlled by an identity subject (i.e., entity)</rdfs:comment>
        <rdfs:label xml:lang="en">controlledBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#controlls -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#controlls">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:comment xml:lang="en">Indicates that an identity subject (i.e., entity) controlls an identity</rdfs:comment>
        <rdfs:label>controlls</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#credential -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#credential">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Credential"/>
        <rdfs:comment xml:lang="en">Indicates that an identity subject (i.e., entity) has credential(s).</rdfs:comment>
        <rdfs:label>credential</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#credentialAttribute -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#credentialAttribute">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Credential"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Attribute"/>
        <rdfs:comment xml:lang="en">Indicates that a credential has attribute(s) to describe a credential, e.g., username/password</rdfs:comment>
        <rdfs:label xml:lang="en">credentialAttribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#credentialFormat -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#credentialFormat">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Credential"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#CredentialFormat"/>
        <rdfs:comment xml:lang="en">Indicates that a credential has its specific format</rdfs:comment>
        <rdfs:label xml:lang="en">credentialFormat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#functional -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#functional">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Functional"/>
        <rdfs:comment xml:lang="en">Indicates that an identifier has its own application-specific use</rdfs:comment>
        <rdfs:label>functional</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#identifier -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#identifier">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:comment xml:lang="en">Indicates that an identity subject (i.e., entity) has identifier(s)</rdfs:comment>
        <rdfs:label>identifier</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#identifierFormat -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#identifierFormat">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#IdentifierFormat"/>
        <rdfs:comment xml:lang="en">Indicates that an identifier has its specific format</rdfs:comment>
        <rdfs:label>format</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#key -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#key">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Attribute"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Key"/>
        <rdfs:comment xml:lang="en">Represents a key of an attribute</rdfs:comment>
        <rdfs:label>key</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#localPart -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#localPart">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#CompositeIdentifier"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:comment xml:lang="en">Indicates the local part of a composite identifier</rdfs:comment>
        <rdfs:label>localPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#proof -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#proof">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Proof"/>
        <rdfs:comment xml:lang="en">Indicates a proof of an assertion, which ensures the integrity and authenticy of the assertion</rdfs:comment>
        <rdfs:label>proof</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#relatesTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#relatesTo">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Credential"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:comment xml:lang="en">Indicates that an assertion or a credential relates to an identifier of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">relatesTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#status -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#status">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Status"/>
        <rdfs:comment xml:lang="en">Indicates the current status of an identity</rdfs:comment>
        <rdfs:label>status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#subAssertion -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#subAssertion">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#AssertionCollection"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:comment xml:lang="en">Indicates the consisting assertion of an assertion collection</rdfs:comment>
        <rdfs:label>sub assertion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#value -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#value">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Attribute"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Value"/>
        <rdfs:comment xml:lang="en">Represents a value of an attribute</rdfs:comment>
        <rdfs:label>value</rdfs:label>
    </owl:ObjectProperty>
    


