        <rdfs:label xml:lang="en">assertionFormat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#controlledBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#controlledBy">
