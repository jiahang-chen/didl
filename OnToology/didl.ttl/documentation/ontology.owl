    <!-- http://xmlns.com/foaf/0.1/Agent -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent">
        <owl:equivalentClass rdf:resource="https://w3id.org/def/didl#IdentitySubject"/>
        <rdfs:comment xml:lang="en">An agent is an entity that can perform actions</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://schema.org/Person -->


    <owl:Class rdf:about="https://schema.org/Person"/>
    


    <!-- https://w3id.org/def/dedl#Action -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Action">
        <rdfs:comment xml:lang="en">An action that can be performed on a resource</rdfs:comment>
        <rdfs:label xml:lang="en">Action</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Resource -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Resource">
        <rdfs:comment xml:lang="en">A resource that can be accessed or manipulated</rdfs:comment>
        <rdfs:label xml:lang="en">Resource</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#AccessCapability -->


    <owl:Class rdf:about="https://w3id.org/def/didl#AccessCapability">
        <rdfs:comment xml:lang="en">A capability that defines what actions can be performed on which resources</rdfs:comment>
        <rdfs:label xml:lang="en">AccessCapability</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Assertion -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Assertion">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Identity"/>
        <owl:disjointWith rdf:resource="https://w3id.org/def/didl#Credential"/>
        <owl:disjointWith rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:comment xml:lang="en">A collection of identity-related attributes asserted by a trusted party</rdfs:comment>
        <rdfs:label xml:lang="en">Assertion</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#AssertionCollection -->


    <owl:Class rdf:about="https://w3id.org/def/didl#AssertionCollection">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:comment xml:lang="en">A specialized assertion consists of multiple assertions</rdfs:comment>
        <rdfs:label xml:lang="en">AssertionCollection</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#AssertionFormat -->


    <owl:Class rdf:about="https://w3id.org/def/didl#AssertionFormat">
