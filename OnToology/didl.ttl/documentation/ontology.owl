        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:range rdf:resource="https://w3id.org/security#Proof"/>
        <rdfs:comment xml:lang="en">Indicates a proof of an assertion, which ensures the integrity and authenticity of the assertion</rdfs:comment>
        <rdfs:label xml:lang="en">proof</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#relatesTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#relatesTo">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/def/didl#Assertion"/>
                    <rdf:Description rdf:about="https://w3id.org/def/didl#Credential"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:comment xml:lang="en">Indicates that an assertion or a credential relates to an identifier of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">relatesTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#status -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#status">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
