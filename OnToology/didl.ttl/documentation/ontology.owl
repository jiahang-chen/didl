    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/def/didl#Assertion -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Assertion">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:comment xml:lang="en">A collection of identity-related attributes asserted by a trusted party</rdfs:comment>
        <rdfs:label xml:lang="en">Assertion</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#AssertionCollection -->


    <owl:Class rdf:about="https://w3id.org/def/didl#AssertionCollection">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Assertion"/>
        <rdfs:comment xml:lang="en">A specialized assertion consists of multiple assertions</rdfs:comment>
        <rdfs:label xml:lang="en">AssertionCollection</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#AssertionFormat -->


    <owl:Class rdf:about="https://w3id.org/def/didl#AssertionFormat">
        <rdfs:comment xml:lang="en">An assertion format that is specified by a standard or a protocol</rdfs:comment>
        <rdfs:label xml:lang="en">AssertionFormat</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Attribute -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Attribute">
        <rdfs:comment xml:lang="en">An identity-related attribute consists of a key-value pair</rdfs:comment>
        <rdfs:label xml:lang="en">Attribute</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#CompositeIdentifier -->


    <owl:Class rdf:about="https://w3id.org/def/didl#CompositeIdentifier">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:comment xml:lang="en">A composite identifier that a hierarchical structure</rdfs:comment>
        <rdfs:label xml:lang="en">CompositeIdentifier</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Credential -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Credential">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:comment xml:lang="en">A proof of an entity&apos;s identity</rdfs:comment>
        <rdfs:label xml:lang="en">Credential</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#CredentialFormat -->


    <owl:Class rdf:about="https://w3id.org/def/didl#CredentialFormat">
        <rdfs:comment xml:lang="en">A credential format that is specified by a standard or a protocol</rdfs:comment>
        <rdfs:label xml:lang="en">CredentialFormat</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Functional -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Functional">
        <rdfs:comment xml:lang="en">A concept that represents the use of  an identifier</rdfs:comment>
        <rdfs:label xml:lang="en">Functional</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Identifier -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Identifier">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:comment xml:lang="en">A unique reference to distinguish one entity from another</rdfs:comment>
        <rdfs:label>Identifier</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#IdentifierFormat -->


    <owl:Class rdf:about="https://w3id.org/def/didl#IdentifierFormat">
        <rdfs:comment xml:lang="en">An identifier format that is specified by a standard or a protocol</rdfs:comment>
        <rdfs:label xml:lang="en">IdentifierFormat</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Identity -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Identity">
        <rdfs:comment xml:lang="en">A digital identity that can be specialized to identifiers, credentials, or assertions</rdfs:comment>
        <rdfs:label xml:lang="en">Identity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#IdentitySubject -->


    <owl:Class rdf:about="https://w3id.org/def/didl#IdentitySubject">
        <rdfs:comment xml:lang="en">An entity that is identified by an identity</rdfs:comment>
        <rdfs:label xml:lang="en">IdentitySubject</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#InherenceBasedCredential -->


    <owl:Class rdf:about="https://w3id.org/def/didl#InherenceBasedCredential">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#CredentialFormat"/>
        <rdfs:comment xml:lang="en">A credential that indicates something you inherently has</rdfs:comment>
        <rdfs:label xml:lang="en">InherenceBasedCredential</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Key -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Key">
        <rdfs:comment xml:lang="en">Relates to the key of an attribute&apos;s key-value pair</rdfs:comment>
        <rdfs:label xml:lang="en">Key</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#KnowledgeBasedCredential -->


    <owl:Class rdf:about="https://w3id.org/def/didl#KnowledgeBasedCredential">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#CredentialFormat"/>
        <rdfs:comment xml:lang="en">A credential that indicates something you know</rdfs:comment>
        <rdfs:label xml:lang="en">KnowledgeBasedCredential</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#PossessionBasedCredential -->


    <owl:Class rdf:about="https://w3id.org/def/didl#PossessionBasedCredential">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#CredentialFormat"/>
        <rdfs:comment xml:lang="en">A credential that indicates something you has</rdfs:comment>
        <rdfs:label xml:lang="en">PossesionBasedCredential</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Proof -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Proof">
        <owl:equivalentClass rdf:resource="https://w3id.org/security#Proof"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/didl#Credential"/>
        <rdfs:comment xml:lang="en">A possession-based credential that uses cryptograhpic method</rdfs:comment>
        <rdfs:label xml:lang="en">Proof</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Status -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Status">
        <rdfs:comment xml:lang="en">Relates to the status of an identity, including active, inactive, or revoked</rdfs:comment>
        <rdfs:label xml:lang="en">Status</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Uniqueness -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Uniqueness">
        <rdfs:comment xml:lang="en">Relates to wheter an identifier is (only) organizationally unique or not</rdfs:comment>
        <rdfs:label xml:lang="en">Uniqueness</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Value -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Value">
        <rdfs:comment xml:lang="en">Relates to the value of an attribute&apos;s key-value pair</rdfs:comment>
        <rdfs:label xml:lang="en">Value</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/security#Proof -->


    <owl:Class rdf:about="https://w3id.org/security#Proof"/>
    


