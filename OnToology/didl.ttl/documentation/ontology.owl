        <owl:equivalentProperty rdf:resource="https://www.w3.org/ns/credentials#issuanceDate"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the issue date of an identity</rdfs:comment>
        <rdfs:label xml:lang="en">issueDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#issuer -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#issuer">
        <owl:equivalentProperty rdf:resource="https://www.w3.org/ns/credentials#issuer"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Indicates the issuer of an identity</rdfs:comment>
        <rdfs:label xml:lang="en">issuer</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#namespace -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#namespace">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Indicates a namespace of an identifier, representing its definition scope</rdfs:comment>
        <rdfs:label xml:lang="en">namespace</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#profile -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#profile">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Indicates an identifier of a DIDL Profile that the identity conforms to</rdfs:comment>
        <rdfs:label xml:lang="en">profile</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#renewableTill -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#renewableTill">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates that the identity can be renewed until the given time</rdfs:comment>
        <rdfs:label xml:lang="en">renewableTill</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#uid -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#uid">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Indicates the unique identifier of an identity itself, e.g., an ID of a verifiable credential</rdfs:comment>
        <rdfs:label xml:lang="en">uid</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#validFrom -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#validFrom">
        <owl:equivalentProperty rdf:resource="https://www.w3.org/ns/credentials#validFrom"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the date from which an identity is valid</rdfs:comment>
        <rdfs:label xml:lang="en">validFrom</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/didl#validTo -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/didl#validTo">
        <owl:equivalentProperty rdf:resource="https://www.w3.org/ns/credentials#validTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the date until which an identity is valid</rdfs:comment>
        <rdfs:label xml:lang="en">validTo</rdfs:label>
    </owl:DatatypeProperty>
    


