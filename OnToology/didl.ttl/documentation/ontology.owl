    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/didl#supportedResource -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/didl#supportedResource">
        <rdfs:domain rdf:resource="https://w3id.org/def/didl#AccessCapability"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Resource"/>
        <rdfs:comment xml:lang="en">Indicates the resources supported by an access capability</rdfs:comment>
        <rdfs:label xml:lang="en">supportedResource</rdfs:label>
    </owl:ObjectProperty>
    


