    <owl:DatatypeProperty rdf:about="https://www.w3.org/ns/credentials#issuanceDate">
        <rdfs:comment xml:lang="en">The date and time when the credential was issued</rdfs:comment>
        <rdfs:label xml:lang="en">issuanceDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.w3.org/ns/credentials#issuer -->


    <owl:DatatypeProperty rdf:about="https://www.w3.org/ns/credentials#issuer">
        <rdfs:comment xml:lang="en">The entity that issued the credential</rdfs:comment>
        <rdfs:label xml:lang="en">issuer</rdfs:label>
    </owl:DatatypeProperty>
    


